@model IEnumerable<QuanLyTour.Models.ChiTietDoanModel>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
<table class="table">
    <tr>
        <th>
            <p>Mã khách hàng</p>
        </th>
        <th>
            <p>Họ tên</p>
        </th>
        <th>
            <p>Số CMND</p>
        </th>
        <th>
            <p>Địa chỉ</p>
        </th>
        <th>
            <p>Giới Tính</p>
        </th>
        <th>
            <p>SĐT</p>
        </th>
        <th>
            <p>Quốc tịch</p>
        </th>
      

        @*<th></th>*@
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaKhachHang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoTen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoCMND)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaChi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GioiTinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SDT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuocTich)
            </td>
          

            @*<td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MaDoan }) |
                @Html.ActionLink("Details", "Details", new { id = item.MaDoan }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MaDoan })
            </td>*@
        </tr>
    }

</table>


